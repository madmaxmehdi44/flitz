{
  "name": "flitz",
  "version": "1.0.0",
  "scripts": {
    "build": "blitz prisma db push --accept-data-loss && blitz build",
    "start:production": "blitz start --port $PORT",
    "dev": "prisma generate && blitz dev",
    "lint:fix": "eslint --fix app integrations pages",
    "lint": "eslint app integrations pages",
    "format": "prettier --write app integrations pages",
    "start": "blitz start",
    "studio": "blitz prisma studio --browser none",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "kill": "killall node -9"
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.2",
    "@blitzjs/next": "2.0.0-beta.2",
    "@blitzjs/rpc": "2.0.0-beta.2",
    "@chakra-ui/react": "2.2.8",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@hookform/resolvers": "2.9.7",
    "@prisma/client": "latest",
    "@sentry/browser": "7.11.1",
    "@sentry/node": "7.11.1",
    "@sentry/tracing": "7.11.1",
    "@types/jest": "28.1.7",
    "avvvatars-react": "0.4.2",
    "blitz": "2.0.0-beta.2",
    "chakra-ui": "0.3.9",
    "firebase": "9.9.2",
    "firebase-admin": "11.0.1",
    "formidable": "2.0.1",
    "framer-motion": "7.2.0",
    "i18next": "21.9.1",
    "i18next-browser-languagedetector": "6.1.5",
    "jdenticon": "3.2.0",
    "next": "latest",
    "prisma": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "7.34.2",
    "react-i18next": "11.18.4",
    "react-icons": "4.4.0",
    "react-textarea-autosize": "^8.3.4",
    "reflect-metadata": "0.1.13",
    "sharp": "0.30.7",
    "tsyringe": "4.7.0",
    "zod": "3.18.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.18.10",
    "@babel/plugin-proposal-private-property-in-object": "7.18.6",
    "@jest/types": "28.1.3",
    "@types/formidable": "2.0.5",
    "@types/react": "18.0.1",
    "@types/sharp": "0.30.5",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "eslint": "8.17.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "4.2.0",
    "pretty-quick": "3.1.3",
    "typescript": "latest"
  },
  "private": true,
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
