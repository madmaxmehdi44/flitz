{
  "name": "flitz",
  "version": "1.0.0",
  "scripts": {
    "build": "blitz build",
    "start:production": "blitz start --port $PORT",
    "dev": "prisma generate && next dev",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx .",
    "format": "prettier --write --ignore-path .gitignore */**/*.{ts,tsx}",
    "start": "blitz start",
    "studio": "blitz prisma studio --browser none",
    "test": "jest",
    "test:watch": "jest --watch",
    "kill": "killall node -9"
  },
  "dependencies": {
    "@blitzjs/auth": "2.0.0-beta.19",
    "@blitzjs/next": "2.0.0-beta.19",
    "@blitzjs/rpc": "2.0.0-beta.19",
    "@chakra-ui/react": "2.4.3",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@hookform/resolvers": "2.9.10",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@prisma/client": "4.7.1",
    "@sentry/browser": "7.23.0",
    "@sentry/node": "7.23.0",
    "@sentry/tracing": "7.23.0",
    "@types/jest": "29.2.4",
    "avvvatars-react": "0.4.2",
    "blitz": "2.0.0-beta.19",
    "cookie-parser": "^1.4.6",
    "firebase": "9.14.0",
    "firebase-admin": "11.3.0",
    "formidable": "2.1.1",
    "framer-motion": "7.6.19",
    "helmet": "^6.0.1",
    "i18next": "22.0.8",
    "i18next-browser-languagedetector": "7.0.1",
    "jdenticon": "3.2.0",
    "next": "13.0.6",
    "prisma": "4.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.40.0",
    "react-i18next": "12.1.0",
    "react-icons": "4.7.1",
    "react-textarea-autosize": "^8.4.0",
    "reflect-metadata": "0.1.13",
    "sharp": "0.31.2",
    "tsyringe": "4.7.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@jest/types": "29.3.1",
    "@types/formidable": "2.0.5",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/sharp": "^0.31.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.0",
    "prettier-plugin-prisma": "4.4.0",
    "typescript": "4.9.3"
  },
  "private": true,
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
