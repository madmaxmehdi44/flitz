{
  "deno.enable": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.exclude": {
    "**/node_modules": true,
    ".blitz": true,
    ".idea": true,
    ".next": true,
    ".tsbuildinfo": true,
    "yarn-error.log": true,
    "yarn.lock": true
  },
  "search.exclude": {
    "yarn.lock": true
  }
}
